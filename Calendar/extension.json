{
    "manifestVersion": 1,
    "id": "team-calendar",
    "version": "0.1.0",
    "publisher": "ms",
    "name": "VSO Team Calendar",
    "description": "Track events important to your team, view and manage days off, quickly see when sprints start and end, and more!",
    "baseUri": "https://vsoalmcalendar-demo.azurewebsites.net/",
    "icons": {
        "default": "images/calendar-logo.png"
    },
    "tags": [
        "Planning",
        "Time tracking",
        "Calendar"
    ],
    "categories": [
        "Planning",
        "Collaboration"
    ],
    "contributions": [
        {
            "id": "calendar",
            "targets": [
                "ms.vss-web.home-hub-group"
            ],
            "type": "ms.vss-web.hub",
            "properties": {
                "name": "Calendar",
                "order": 22,
                "uri": "calendar.html"
            }
        },
        {
            "id": "freeForm",
            "targets": [
                ".calendar"
            ],
            "type": "ms.vss-web.service",
            "properties": {
                "uri": "calendarServices.html"
            }
        },
        {
            "id": "daysOff",
            "targets": [
                ".calendar"
            ],
            "type": "ms.vss-web.service",
            "properties": {
                "uri": "calendarServices.html"
            }
        },
        {
            "id": "iterations",
            "targets": [
                ".calendar"
            ],
            "type": "ms.vss-web.service",
            "properties": {
                "uri": "calendarServices.html"
            }
        }
    ]
}